<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.miniproject.mapper.MemberMapper">

    <insert id="regMember" parameterType="com.spring.miniproject.domain.ShopMember">
        insert into shopmember (uid, pw, uname, uaddress) values (#{uid}, #{pw}, #{uname},#{uaddress})
    </insert>



</mapper>